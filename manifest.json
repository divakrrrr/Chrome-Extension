{
    "manifest_version": 3,
    "name": "URL to QR Code",
    "version": "1.0",
    "description": "Generate a QR Code from the entered URL",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  